{
  "name": "@veobot/eslint-config",
  "version": "1.2.1",
  "description": "Veo Robotics ESLint rules for all JavaScript / TypeScript projects",
  "author": {
    "name": "Ty-Lucas Kelley",
    "email": "ty@veobot.com"
  },
  "homepage": "https://github.com/veobot/eslint-config",
  "bugs": "https://github.com/veobot/eslint-config/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/veobot/eslint-config"
  },
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-config",
    "javascript",
    "typescript",
    "vue",
    "es2019",
    "nodejs",
    "lint"
  ],
  "scripts": {
    "preinstall": "node -e \"if (!~process.env.npm_execpath.indexOf('yarn')) throw new Error('Install dependencies with yarn, not npm')\"",
    "lint": "yarn lint:js && yarn lint:md && yarn lint:unused",
    "lint:js": "eslint index.js rules/**/*.js test/**/pass.* test/*.js",
    "lint:unused": "eslint --report-unused-disable-directives index.js rules/**/*",
    "lint:md": "markdownlint README.md CHANGELOG.md",
    "pretest": "yarn lint",
    "test": "jest",
    "find-rules": "eslint-find-rules --unused",
    "prepublishOnly": "yarn test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test",
      "pre-push": "yarn test"
    }
  },
  "main": "index.js",
  "engines": {
    "node": ">= 8",
    "yarn": ">= 1.13.0"
  },
  "peerDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.3",
    "@babel/preset-env": "7.3.1",
    "@typescript-eslint/eslint-plugin": "1.4.0",
    "@typescript-eslint/parser": "1.4.0",
    "@vue/eslint-config-typescript": "4.0.0",
    "eslint": "5.14.1",
    "eslint-import-resolver-typescript": "1.1.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jest": "22.3.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-vue": "5.2.2",
    "typescript": "3.3.3",
    "vue-eslint-parser": "6.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.3",
    "@babel/preset-env": "7.3.1",
    "@typescript-eslint/eslint-plugin": "1.4.0",
    "@typescript-eslint/parser": "1.4.0",
    "@vue/eslint-config-typescript": "4.0.0",
    "eslint": "5.14.1",
    "eslint-find-rules": "3.3.1",
    "eslint-import-resolver-typescript": "1.1.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jest": "22.3.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-vue": "5.2.2",
    "husky": "1.3.1",
    "jest": "24.1.0",
    "markdownlint-cli": "0.13.0",
    "shelljs": "0.8.3",
    "typescript": "3.3.3",
    "typescript-eslint-parser": "^22.0.0",
    "vue": "2.6.6",
    "vue-eslint-parser": "6.0.2"
  }
}
